                        {%- if layout == 'left' -%}
                        <div class="halo-grid-row row-1">
                        {%- endif -%}
                            <div class="item item-1">
                                <div class="banner-item">
                                    <div class="img-box">
                                        <loading-banner class="loading-banner">
                                            {%- if image_1 != blank -%}
                                                <{% if link_1 != blank %}a{% else %}div{% endif %} class="image image-{{ image_effect }} image-border-{{ image_border_radius }}" {% if link_1 != blank %} href="{{ link_1 }}"{% endif %} style="padding-top: {{ 1 | divided_by: image_1.aspect_ratio | times: 100 | append: '%' }}">
                                                    <img srcset="{{ image_1 | img_url: 'master' }} 1x, {{ image_1 | img_url: 'master', scale: 2 }} 2x"
                                                        src="{{ image_1 | img_url: 'master' }}"
                                                        loading="lazy"
                                                        alt="{{ image_1.alt | default: shop.name | escape }}">
                                                    {%- if image_effect == 'liquid' -%}
                                                        <span class="hover"></span>
                                                    {%- endif -%}
                                                </{% if link_1 != blank %}a{% else %}div{% endif %}>
                                            {%- else -%}
                                                <div class="image">
                                                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                                </div>
                                            {%- endif -%}
                                        </loading-banner>
                                    </div>
                                    {%- if title_1 != blank or link_text_1 != blank -%}
                                        <div class="content-box text-{{ align }}">
                                            {%- if title_1 != blank -%}
                                                <h4 class="banner-title typography__mobile typography__tablet typography__desktop">
                                                    <{% if link_1 != blank %}a{% else %}span{% endif %} {% if link_1 != blank %} href="{{ link_1 }}"{% endif %}>
                                                        {{ title_1 | escape }}
                                                    </{% if link_1 != blank %}a{% else %}span{% endif %}>
                                                </h4>
                                            {%- endif -%}
                                            {%- if link_text_1 != blank -%}
                                                <{% if link_1 != blank %}a{% else %}div{% endif %} {% if link_1 != blank %} href="{{ link_1 }}"{% endif %} class="banner-link link link-underline text-underline typography__link">
                                                    <span class="text">{{ link_text_1 | escape }}</span>
                                                </{% if link_1 != blank %}a{% else %}div{% endif %}>
                                            {%- endif -%}
                                        </div>
                                    {%- endif -%}
                                </div>
                            </div>
                        {%- if layout == 'left' -%}
                        </div>
                        <div class="halo-grid-row row-2">
                        {%- endif -%}
                            <div class="item item-2">
                                <div class="banner-item">
                                    <div class="img-box">
                                        <loading-banner class="loading-banner">
                                            {%- if image_2 != blank -%}
                                                <{% if link_2 != blank %}a{% else %}div{% endif %} class="image image-{{ image_effect }} image-border-{{ image_border_radius }}" {% if link_2 != blank %} href="{{ link_2 }}"{% endif %} style="padding-top: {{ 1 | divided_by: image_2.aspect_ratio | times: 100 | append: '%' }}">
                                                    <img srcset="{{ image_2 | img_url: 'master' }} 1x, {{ image_2 | img_url: 'master', scale: 2 }} 2x"
                                                        src="{{ image_2 | img_url: 'master' }}"
                                                        loading="lazy"
                                                        alt="{{ image_2.alt | default: shop.name | escape }}">
                                                    {%- if image_effect == 'liquid' -%}
                                                        <span class="hover"></span>
                                                    {%- endif -%}
                                                </{% if link_2 != blank %}a{% else %}div{% endif %}>
                                            {%- else -%}
                                                <div class="image">
                                                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                                </div>
                                            {%- endif -%}
                                        </loading-banner>
                                    </div>
                                    {%- if title_2 != blank or link_text_2 != blank -%}
                                        <div class="content-box text-{{ align }}">
                                            {%- if title_2 != blank -%}
                                                <h4 class="banner-title typography__mobile typography__tablet typography__desktop">
                                                    <{% if link_2 != blank %}a{% else %}span{% endif %} {% if link_2 != blank %} href="{{ link_2 }}"{% endif %}>
                                                        {{ title_2 | escape }}
                                                    </{% if link_2 != blank %}a{% else %}span{% endif %}>
                                                </h4>
                                            {%- endif -%}
                                            {%- if link_text_2 != blank -%}
                                                <{% if link_2 != blank %}a{% else %}div{% endif %} {% if link_2 != blank %} href="{{ link_2 }}"{% endif %} class="banner-link link link-underline text-underline typography__link">
                                                    <span class="text">{{ link_text_2 | escape }}</span>
                                                </{% if link_2 != blank %}a{% else %}div{% endif %}>
                                            {%- endif -%}
                                        </div>
                                    {%- endif -%}
                                </div>
                            </div>
                            <div class="item item-3">
                                <div class="banner-item">
                                    <div class="img-box">
                                        <loading-banner class="loading-banner">
                                            {%- if image_3 != blank -%}
                                                <{% if link_3 != blank %}a{% else %}div{% endif %} class="image image-{{ image_effect }} image-border-{{ image_border_radius }}" {% if link_3 != blank %} href="{{ link_3 }}"{% endif %} style="padding-top: {{ 1 | divided_by: image_3.aspect_ratio | times: 100 | append: '%' }}">
                                                    <img srcset="{{ image_3 | img_url: 'master' }} 1x, {{ image_3 | img_url: 'master', scale: 2 }} 2x"
                                                        src="{{ image_3 | img_url: 'master' }}"
                                                        loading="lazy"
                                                        alt="{{ image_3.alt | default: shop.name | escape }}">
                                                    {%- if image_effect == 'liquid' -%}
                                                        <span class="hover"></span>
                                                    {%- endif -%}
                                                </{% if link_3 != blank %}a{% else %}div{% endif %}>
                                            {%- else -%}
                                                <div class="image">
                                                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                                </div>
                                            {%- endif -%}
                                        </loading-banner>
                                    </div>
                                    {%- if title_3 != blank or link_text_3 != blank -%}
                                        <div class="content-box text-{{ align }}">
                                            {%- if title_3 != blank -%}
                                                <h4 class="banner-title typography__mobile typography__tablet typography__desktop">
                                                    <{% if link_3 != blank %}a{% else %}span{% endif %} {% if link_3 != blank %} href="{{ link_3 }}"{% endif %}>
                                                        {{ title_3 | escape }}
                                                    </{% if link_3 != blank %}a{% else %}span{% endif %}>
                                                </h4>
                                            {%- endif -%}
                                            {%- if link_text_3 != blank -%}
                                                <{% if link_3 != blank %}a{% else %}div{% endif %} {% if link_3 != blank %} href="{{ link_3 }}"{% endif %} class="banner-link link link-underline text-underline typography__link">
                                                    <span class="text">{{ link_text_3 | escape }}</span>
                                                </{% if link_3 != blank %}a{% else %}div{% endif %}>
                                            {%- endif -%}
                                        </div>
                                    {%- endif -%}
                                </div>
                            </div>
                        {%- if layout == 'left' -%}
                        </div>
                        {%- endif -%}
                    </div>
                    <div class="halo-grid-item halo-grid-item{{ layout | capitalize }}">
                        {%- if layout == 'right' -%}
                        <div class="halo-grid-row row-1">
                        {%- endif -%}
                        <div class="item item-4">
                            <div class="banner-item">
                                <div class="img-box">
                                    <loading-banner class="loading-banner">
                                        {%- if image_4 != blank -%}
                                            <{% if link_4 != blank %}a{% else %}div{% endif %} class="image image-{{ image_effect }} image-border-{{ image_border_radius }}" {% if link_4 != blank %} href="{{ link_4 }}"{% endif %} style="padding-top: {{ 1 | divided_by: image_4.aspect_ratio | times: 100 | append: '%' }}">
                                                <img srcset="{{ image_4 | img_url: 'master' }} 1x, {{ image_4 | img_url: 'master', scale: 2 }} 2x"
                                                    src="{{ image_4 | img_url: 'master' }}"
                                                    loading="lazy"
                                                    alt="{{ image_4.alt | default: shop.name | escape }}">
                                                {%- if image_effect == 'liquid' -%}
                                                    <span class="hover"></span>
                                                {%- endif -%}
                                            </{% if link_4 != blank %}a{% else %}div{% endif %}>
                                        {%- else -%}
                                            <div class="image">
                                                {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                            </div>
                                        {%- endif -%}
                                    </loading-banner>
                                </div>
                                {%- if title_4 != blank or link_text_4 != blank -%}
                                    <div class="content-box text-{{ align }}">
                                        {%- if title_4 != blank -%}
                                            <h4 class="banner-title typography__mobile typography__tablet typography__desktop">
                                                <{% if link_4 != blank %}a{% else %}span{% endif %} {% if link_4 != blank %} href="{{ link_4 }}"{% endif %}>
                                                    {{ title_4 | escape }}
                                                </{% if link_4 != blank %}a{% else %}span{% endif %}>
                                            </h4>
                                        {%- endif -%}
                                        {%- if link_text_4 != blank -%}
                                            <{% if link_4 != blank %}a{% else %}div{% endif %} {% if link_4 != blank %} href="{{ link_4 }}"{% endif %} class="banner-link link link-underline text-underline typography__link">
                                                <span class="text">{{ link_text_4 | escape }}</span>
                                            </{% if link_4 != blank %}a{% else %}div{% endif %}>
                                        {%- endif -%}
                                    </div>
                                {%- endif -%}
                            </div>
                        </div>
                        {%- if layout == 'right' -%}
                        </div>
                        <div class="halo-grid-row row-2">
                        {%- endif -%}
                        <div class="item item-5">
                            <div class="banner-item">
                                <div class="img-box">
                                    <loading-banner class="loading-banner">
                                        {%- if image_5 != blank -%}
                                            <{% if link_5 != blank %}a{% else %}div{% endif %} class="image image-{{ image_effect }} image-border-{{ image_border_radius }}" {% if link_5 != blank %} href="{{ link_5 }}"{% endif %} style="padding-top: {{ 1 | divided_by: image_5.aspect_ratio | times: 100 | append: '%' }}">
                                                <img srcset="{{ image_5 | img_url: 'master' }} 1x, {{ image_5 | img_url: 'master', scale: 2 }} 2x"
                                                    src="{{ image_5 | img_url: 'master' }}"
                                                    loading="lazy"
                                                    alt="{{ image_5.alt | default: shop.name | escape }}">
                                                {%- if image_effect == 'liquid' -%}
                                                    <span class="hover"></span>
                                                {%- endif -%}
                                            </{% if link_5 != blank %}a{% else %}div{% endif %}>
                                        {%- else -%}
                                            <div class="image">
                                                {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                            </div>
                                        {%- endif -%}
                                    </loading-banner>
                                </div>
                                {%- if title_5 != blank or link_text_5 != blank -%}
                                    <div class="content-box text-{{ align }}">
                                        {%- if title_5 != blank -%}
                                            <h4 class="banner-title typography__mobile typography__tablet typography__desktop">
                                                <{% if link_5 != blank %}a{% else %}span{% endif %} {% if link_5 != blank %} href="{{ link_5 }}"{% endif %}>
                                                    {{ title_5 | escape }}
                                                </{% if link_5 != blank %}a{% else %}span{% endif %}>
                                            </h4>
                                        {%- endif -%}
                                        {%- if link_text_5 != blank -%}
                                            <{% if link_5 != blank %}a{% else %}div{% endif %} {% if link_5 != blank %} href="{{ link_5 }}"{% endif %} class="banner-link link link-underline text-underline typography__link">
                                                <span class="text">{{ link_text_5 | escape }}</span>
                                            </{% if link_5 != blank %}a{% else %}div{% endif %}>
                                        {%- endif -%}
                                    </div>
                                {%- endif -%}
                            </div>
                        </div>
                        <div class="item item-6">
                            <div class="banner-item">
                                <div class="img-box">
                                    <loading-banner class="loading-banner">
                                        {%- if image_6 != blank -%}
                                            <{% if link_6 != blank %}a{% else %}div{% endif %} class="image image-{{ image_effect }} image-border-{{ image_border_radius }}" {% if link_6 != blank %} href="{{ link_6 }}"{% endif %} style="padding-top: {{ 1 | divided_by: image_6.aspect_ratio | times: 100 | append: '%' }}">
