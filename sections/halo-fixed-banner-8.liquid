        "settings": [
            {
                "type": "header",
                "content": "t:sections.fixed-banner-8.settings.header__1.content"
            },
            {
                "type": "checkbox",
                "id": "show_fixed_banner",
                "label": "t:sections.fixed-banner-8.settings.general.label__1",
                "default": true
            },
            {
                "type": "select",
                "id": "container",
                "label": "t:sections.general.settings.container.label__1",
