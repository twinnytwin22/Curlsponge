{%- liquid
    assign container = section.settings.container
    assign show_line_separator = section.settings.show_line_separator
    assign show_header_transparent = section.settings.show_header_transparent
    if show_header_transparent
        assign header_bg = section.settings.header_bg
        assign color_modify = section.settings.color_modify | times: 0.01
        assign color_modify_2 = section.settings.color_modify_2 | times: 0.01
        assign color_modify_1_text = section.settings.color_modify_1_text
        assign color_modify_1_icon = section.settings.color_modify_1_icon
        assign color_modify_2_text = section.settings.color_modify_2_text
        assign color_modify_2_icon = section.settings.color_modify_2_icon
    endif
    if section.blocks.size > 0
        assign block_language_currency = section.blocks | where: "type", "language_currency"
        assign block_logo = section.blocks | where: "type", "logo"
        assign block_function_group = section.blocks | where: "type", "function-group"
        assign block_main_menu = section.blocks | where: "type", "main-menu"
        for block in block_language_currency
                assign show_language_currency = block.settings.show_language_currency
                if show_language_currency
                    assign shoose_style_currrency = block.settings.shoose_style_currrency
                endif
                assign show_custom_social_media_link = block.settings.show_custom_social_media_link
                assign text_align_3 = block.settings.text_align
            endfor
        for block in block_logo
            assign logo_type = block.settings.logo_type
            assign logo_text_transform = block.settings.logo_text_transform
            if logo_type == 'image'
                assign image = block.settings.logo
                assign logo_transparent = block.settings.logo_transparent
                assign image_size = block.settings.logo_width | append: 'x'
                if request.page_type == 'index' and logo_transparent != blank and show_header_transparent
                    assign has_logo_transparent = true
                endif
                assign logo_width = block.settings.logo_width | append: 'px'
            elsif logo_type == 'text'
                assign logo_text = block.settings.logo_text
            elsif logo_type == 'icon'
                assign logo_icon = block.settings.logo_icon
            endif
        endfor
        for block in block_function_group
            assign show_wishlist = block.settings.show_wishlist
            assign show_customer = block.settings.show_customer
            assign show_cart = block.settings.show_cart
            assign show_search = block.settings.show_search
            if show_search
                assign header_search_style = block.settings.header_search_style
            endif
            assign show_bg_cart = block.settings.show_bg_cart
            if show_bg_cart
                assign cart_item_color = block.settings.cart_item_color
                assign cart_item_background = block.settings.cart_item_background
            endif
            assign show_custom_link = block.settings.show_custom_link
            if show_custom_link
                assign custom_link_text = block.settings.custom_link_text
                assign custom_link_url = block.settings.custom_link_url
            endif
            assign style_function_group = block.settings.style_function_group
            assign align_function_group = block.settings.align_function_group
        endfor
        for block in block_main_menu
            assign menu = block.settings.menu
            assign show_text_underline = block.settings.show_text_underline
            if show_text_underline 
                assign text_underline = block.settings.text_underline | append: 'px'
            endif
            assign letter_spacing  = block.settings.letter_spacing | times: 0.001 | append: 'em'
            assign show_megamenu = block.settings.show_megamenu
            assign show_resize_menu = block.settings.show_resize_menu
            assign align = block.settings.align
        endfor
    endif
-%}

{%- if show_megamenu -%}
    <noscript>{{ 'component-megamenu.css' | asset_url | stylesheet_tag }}</noscript>
{%- endif -%}
<link rel="stylesheet" href="{{ 'component-megamenu.css' | asset_url }}" media="all" onload="this.media='all'">

<{% if section.settings.sticky_header_type != 'none' %}sticky-header-full-elements data-sticky-type="{{ section.settings.sticky_header_type }}"{% else %}div{% endif %} id="shopify-section__header-nav-full-elements header-nav-full-elements-{{ section.id }}" class="header-wrapper header-group">
    <header class="header header-nav header-nav-full-elements header-inline style-search-{{ header_search_style }}{% if show_header_transparent %} transparent background-transparent-{{ section.settings.style_bg_transparent }}{% endif %}"
        data-url-style-menu="{% if show_megamenu %}{{ 'component-megamenu.css' | asset_url }}{% endif %}" 
        style="--header-bg: {{ section.settings.header_bg }};
            --header-color: {{ section.settings.header_color }};
            --logo-width: {{ logo_width }};{% if settings.show_dark_mode %}--dark-mode-header-bg: {{ section.settings.header_bg }};--dark-mode-header-color: {{ section.settings.header_color }};{% endif %}
        {% if show_line_separator %}
            --line-separator-color: {{ section.settings.line_separator_color }};
        {% endif %}
        {% if show_header_transparent %}
            --color-modify: {{ header_bg | color_modify: 'alpha', color_modify }};
            --color-modify-2: {{ header_bg | color_modify: 'alpha', color_modify_2 }};
            --color-modify-1-text: {{ color_modify_1_text }};
            --color-modify-1-icon: {{ color_modify_1_icon }};
            --color-modify-2-text: {{ color_modify_2_text }};
            --color-modify-2-icon: {{ color_modify_2_icon }};
            --icon-color-transparent: {{ section.settings.icon_color_transparent }};
        {% endif %}"
    >
        {%- if block_language_currency.size > 0 or block_logo.size > 0 or block_function_group.size > 0 -%}
            <div class="header-top{% if show_line_separator %} header-wrapper--border{% endif %} element-spacing"
                style="--spacing-top: {{ section.settings.padding_top | append: 'px' }};
                --spacing-bottom: {{ section.settings.padding_bottom | append: 'px' }};
                --spacing-top-mobile: {{ section.settings.padding_top | times: 0.5 | round: 0 | append: 'px' }};
                --spacing-bottom-mobile: {{ section.settings.padding_bottom | times: 0.5 | round: 0 | append: 'px' }};
                --spacing-top-tablet: {{ section.settings.padding_top | times: 0.75 | round: 0 | append: 'px' }};
                --spacing-bottom-tablet: {{ section.settings.padding_bottom | times: 0.75 | round: 0 | append: 'px' }};"
            >
                <div class="container container-{{ container }}">
                    <div class="header-top--wrapper">
                        {%- for block in section.blocks -%}
                            {%- case block.type -%}
                                {%- when 'language_currency' -%}
                                    <div class="header-nav__language header-top-left text-{{ text_align_3 }}">
                                        {%- if show_language_currency -%}
                                            {%- if settings.enable_multilang_shopify or settings.enable_currencies -%}
                                                <div class="header-language_currency{% if show_custom_social_media_link %} header-top-leftItem{% endif %} header__currencyItem wrapper_language_currency style-{{ shoose_style_currrency }} text-{{ text_align_3 }}">
                                                    <language-currency class="language-currency--wrapper position-{{ text_align_3 }}">
                                                        <div class="toggle-language-currency" data-language-currency-toggle>
                                                            <div class="top-language-currency justify-end">
                                                                {%- unless shoose_style_currrency == '2' -%}
                                                                    {%- if settings.enable_multilang_shopify -%}
                                                                        {% render 'halo-top-language' %}
                                                                        {%- if settings.enable_auto_currency -%}
                                                                            <span class="dropdown-icon" role="button" aria-label="{{ 'accessibility.dropdown' | t }}" tabindex="0">
                                                                                {% render 'icon-sort-down' %}
                                                                            </span>
                                                                        {%- endif -%}
                                                                    {%- endif -%}
                                                                {%- endunless -%}
                                                                {%- if settings.enable_currencies -%}
                                                                    {% render 'halo-top-currency' %}
                                                                {%- endif -%}
                                                                {%- if shoose_style_currrency == '2' -%}
                                                                    {%- if settings.enable_multilang_shopify -%}
                                                                        {% render 'halo-top-language' %}
                                                                        {%- if settings.enable_auto_currency -%}
                                                                            <span class="dropdown-icon" role="button" aria-label="{{ 'accessibility.dropdown' | t }}" tabindex="0">
                                                                                {% render 'icon-sort-down' %}
                                                                            </span>
                                                                        {%- endif -%}
                                                                    {%- endif -%}
                                                                {%- endif -%}
                                                                {%- unless settings.enable_auto_currency -%}
                                                                    <span class="dropdown-icon" role="button" aria-label="{{ 'accessibility.dropdown' | t }}" tabindex="0">
                                                                        {%- if shoose_style_currrency == '2' -%}
                                                                            {% render 'icon-caret' %}
                                                                        {%- else -%}
                                                                            {% render 'icon-sort-down' %}
                                                                        {%- endif -%}
                                                                    </span>
                                                                {%- endunless -%}
                                                            </div>
                                                        </div>
                                                        <div class="dropdown-language-currency custom-scrollbar" data-language-currency-dropdown>
                                                            {%- if settings.enable_multilang_shopify -%}
                                                                {% render 'halo-language' %}
                                                            {%- endif -%}
                                                            {%- if settings.enable_currencies  -%}
                                                                {% render 'halo-currency' %}
                                                            {%- endif -%} 
                                                        </div>
                                                    </language-currency>
                                                </div>
                                            {%- endif -%}
                                        {%- endif -%}
                                        {%- if show_custom_social_media_link -%}
                                            <div class="header-top-leftItem header__inline-social text-{{ text_align_3 }}">
                                                <ul class="list-social list-unstyled" role="list">
                                                    {%- for i in (1..3) -%}
                                                        {% capture cap_social_icon %}social_media_icon_{{ i }}{% endcapture %}
                                                        {% capture cap_social_link %}social_media_link_{{ i }}{% endcapture %}
                                                        {%- liquid
                                                            assign social_media_icon = block.settings[cap_social_icon]
                                                            assign social_media_link = block.settings[cap_social_link]
                                                        -%}
                                                        {%- if social_media_icon != blank -%}
                                                            <li>
                                                                <a href="{{ social_media_link }}" class="link">
                                                                    {{ social_media_icon }}
                                                                    <span class="visually-hidden">{{ shop.name }}</span>
                                                                </a>
                                                            </li>
                                                        {%- endif -%}
                                                    {%- endfor -%}
                                                </ul>
                                            </div>
                                        {%- endif -%}
                                    </div>
                                {%- when 'logo' -%}
                                    <div class="header-nav__logo header-top-center text-center" style="--logo-color: {{ block.settings.logo_color }};--logo-font-size: {{ block.settings.logo_font_size | append: 'px' }};--logo-font-weight: {{ block.settings.logo_font_weight }};{% if logo_type == 'icon' %}--logo-icon-width: {{ block.settings.logo_icon_width | append: 'px' }};--logo-icon-height: {{ block.settings.logo_icon_height | append: 'px' }};{% endif %}">
                                        <{% if request.page_type == 'index' %}h1{% else %}div{% endif %} class="header__heading">
                                            <a href="{{ routes.root_url }}" class="header__heading-link focus-inset">
                                                {%- if logo_type == 'image' -%}
                                                    {%- if image != blank -%}
                                                        <img
                                                            srcset="{{ image | img_url: image_size }} 1x, {{ image | img_url: image_size, scale: 2 }} 2x"
                                                            src="{{ image | img_url: image_size }}"
                                                            loading="lazy"
                                                            class="header__heading-logo{% if request.page_type == 'index' and has_logo_transparent %} header__transparent-logo{% endif %}"
                                                            alt="{{ image.alt | default: shop.name | escape }}"
                                                            width="{{ image.width | replace: 'px', '' }}"
                                                            height="{{ image.height | replace: 'px', '' }}"
                                                        >
                                                    {%- else -%}
                                                        <span class="h2 {{ logo_text_transform }}">{{ shop.name }}</span>
                                                    {%- endif -%}
                                                {%- elsif logo_type == 'text' -%}
                                                    <span class="h2 {{ logo_text_transform }}">
                                                        {% if logo_text != blank %}{{ logo_text }}{% else %}{{ shop.name }}{% endif %}
                                                    </span>
                                                {%- elsif logo_type == 'icon' -%}
                                                    {%- if logo_icon != blank -%}
                                                        {{ logo_icon }}
                                                    {%- else -%}
                                                        <span class="h2 {{ logo_text_transform }}">{{ shop.name }}</span>
                                                    {%- endif -%}
                                                {%- endif -%}
                                                {%- if request.page_type == 'index' and logo_transparent != blank and show_header_transparent -%}
                                                    <img
                                                        srcset="{{ logo_transparent | img_url: image_size }} 1x, {{ image | img_url: image_size, scale: 2 }} 2x"
                                                        src="{{ logo_transparent | img_url: image_size }}"
                                                        loading="lazy"
                                                        class="header__heading-logo"
                                                        alt="{{ logo_transparent.alt | default: shop.name | escape }}"
                                                        width="{{ logo_transparent.width | replace: 'px', '' }}"
                                                        height="{{ logo_transparent.height | replace: 'px', '' }}"
                                                    >
                                                {%- endif -%}
                                            </a>
                                        </{% if request.page_type == 'index' %}h1{% else %}div{% endif %}>
                                    </div>
                                {%- when 'function-group' -%}
                                    <div class="header-nav__icons header-top-right header__icons--margin text-{{ align_function_group }} style-{{ style_function_group }} clearfix" style="--margin-left: {{ block.settings.margin_left | append: 'px' }};--function-group-size: {{ block.settings.function_group_size | append: 'px' }};--cart-count-background: {{ block.settings.cart_count_background }};--cart-count-color: {{ block.settings.cart_count_color }};">
                                        {%- if show_custom_link and custom_link_text != blank -%}
                                            <div class="header__iconItem header__inline-custom_link" style="{% if block.settings.lv_1_font == 'font_1' %}--lv-1-font: var(--font-1-family);{% elsif block.settings.lv_1_font == 'font_2' %}--lv-1-font: var(--font-2-family);{% elsif block.settings.lv_1_font == 'font_3' %}--lv-1-font: var(--font-3-family);{% endif %}--lv-1-font-size: {{ block.settings.lv_1_font_size | append: 'px' }};--lv-1-font-weight: {{ block.settings.lv_1_font_weight }};--lv-1-text-transform: {{ block.settings.lv_1_text_transform }};--lv-1-color: {{ block.settings.lv_1_color }};">
                                                <{% if custom_link_url != blank %}a{% else %}div{% endif %} class="custom-link top-custom-link link link-underline link-effect-1 h-link" {% if custom_link_url != blank %} href="{{ custom_link_url }}"{% endif %}>
                                                    <span class="custom-link--text text">
                                                        {{ custom_link_text | escape }}
                                                    </span>
                                                </{% if custom_link_url != blank %}a{% else %}div{% endif %}>
                                            </div>
                                        {%- endif -%}
                                        {%- if show_search -%}
                                            {%- if header_search_style == 'icon' -%}
                                                <div class="header__iconItem header__search show-icon">
                                                    <div class="header__seach--details header__search-icon">
                                                        {%- liquid
                                                            if style_function_group == '2' or style_function_group == '6' or style_function_group == '7'
                                                                assign icon = '4'
                                                            elsif style_function_group == '3'
                                                                assign icon = '3'
                                                            elsif style_function_group == '4' or style_function_group == '5'
                                                                assign icon = '5'
                                                            elsif style_function_group == '8'
                                                                assign icon = '2'
                                                            else
                                                                assign icon = '1'
                                                            endif
                                                        -%}
                                                        {% render 'halo-search-layout-icon',
                                                            icon_search: icon,
                                                            container: block.settings.search_container,
                                                            search_custom_class: 'search-modal__form--custom',
                                                            has_close: false
                                                        %}
                                                    </div>
                                                </div>
                                            {%- else -%}
                                                <details-modal class="header__iconItem header__search{% unless header_search_style == 'full-2' %} header__search--custom style-{{ header_search_style }}{% endunless %} show-full"
                                                    style="--search_bg: {{ block.settings.search_bg }};
                                                        --search_color: {{ block.settings.search_color }};
                                                        --search_border: {{ block.settings.search_border }};
                                                        --search_icon_color: {{ block.settings.search_icon_color }};
                                                        --search_button_bg_color: {{ block.settings.search_button_bg_color }};"
                                                >
                                                    {%- if settings.show_predictive_search -%}
                                                        <predictive-search data-loading-text="{{ 'accessibility.loading' | t }}" data-product-to-show="{{ settings.quick_search_to_show }}" class="header__seach--details predictive-search-form">
                                                    {%- endif -%}
                                                        <details class="header__seach--details">
                                                            {%- liquid
                                                                if style_function_group == '6' or style_function_group == '7'
                                                                    assign icon = '4'
                                                                elsif style_function_group == '10'
                                                                    assign icon = '2'
                                                                elsif style_function_group == '11'
                                                                    assign icon = '3'
                                                                else
                                                                    assign icon = '1'
                                                                endif
                                                            -%}
                                                            {% render 'halo-search-layout-full',
                                                                icon_search: icon,
                                                                container: block.settings.search_container,
                                                                search_custom_class: 'search-modal__form--mobile',
                                                                has_close: true
                                                            %}
                                                        </details>
                                                    {%- if settings.show_predictive_search -%}
                                                        </predictive-search>
                                                    {%- endif -%}
                                                </details-modal>
                                            {%- endif -%}
                                        {%- endif -%}
                                        {%- if show_customer or show_wishlist or show_cart -%}
                                            <div class="header__iconItem header__group">
                                                {%- if shop.customer_accounts_enabled and show_customer -%}
                                                    <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" class="header__icon header__icon--account link focus-inset" 
                                                        {% if routes.account_login_url == '/account/login' %}
                                                            {% if settings.customer_login_type == 'popup' %} data-open-auth-popup{% else %} data-open-auth-sidebar{% endif %}
                                                        {% endif %}
                                                    >
                                                        {%- liquid
                                                            if style_function_group == '3' or style_function_group == '7'
                                                                assign icon = '5'
                                                            elsif style_function_group == '4' or style_function_group == '5'
                                                                assign icon = '6'
                                                            elsif style_function_group == '12'
                                                                assign icon = '7'
                                                            elsif style_function_group == '6' or style_function_group == '9'
                                                                assign icon = '3'
                                                            elsif style_function_group == '8'
                                                                assign icon = '2'
                                                            else
                                                                assign icon = '1'
                                                            endif
                                                        -%}
                                                        {% render 'icon-account',
                                                            icon_account: icon
                                                        %}
                                                        <span class="visually-hidden">
                                                            {%- liquid
                                                                if customer
                                                                    echo 'customer.account_fallback' | t
                                                                else
                                                                    echo 'customer.log_in' | t
                                                                endif
                                                            -%}
                                                        </span>
                                                    </a>
                                                {%- endif -%}
                                                {%- if settings.show_wishlist and show_wishlist -%}
                                                    <a href="{{ pages['wish-list'].url }}" class="header__icon header__icon--wishlist link focus-inset">
                                                        {%- liquid
                                                            if style_function_group == '3'
                                                                assign icon = '5'
                                                            elsif style_function_group == '4' or style_function_group == '5'
                                                                assign icon = '7'
                                                            elsif style_function_group == '6'
                                                                assign icon = '4'
                                                            elsif style_function_group == '8' or style_function_group == '9'
                                                                assign icon = '2'
                                                            elsif style_function_group == '10'
                                                                assign icon = '3'
                                                            elsif style_function_group == '12'
                                                                assign icon = '8'
                                                            else
                                                                assign icon = '1'
                                                            endif
                                                        -%}
                                                        {% render 'icon-heart',
                                                            icon_heart: icon
                                                        %}
                                                        <span class="visually-hidden">
                                                            {{ 'wishlist.general.title' | t }}
                                                        </span>
                                                        <div class="wishlist-count-bubble">
                                                            <span class="text" aria-hidden="true" data-wishlist-count>0</span>
                                                        </div>
                                                    </a>
                                                {%- endif -%}
                                                {%- if show_cart -%}
                                                    <a {% if settings.show_quick_cart %}{% if settings.quick_cart_type == 'popup' %} data-open-cart-popup href="javascript:void(0)"{% else %}data-cart-sidebar href="javascript:void(0)"{% endif %}{% else %}href="{{ routes.cart_url }}"{% endif %} class="header__icon header__icon--cart link focus-inset{% if show_bg_cart %} bg-cart{% endif %}" id="cart-icon-bubble" role="button"{% if show_bg_cart %} style="--cart-item-color: {{ cart_item_color }}; --cart-item-background: {{ cart_item_background }};"{% endif %}>
                                                        {%- liquid 
                                                            if style_function_group == '1' or style_function_group == '8'
                                                                assign icon = '1'
                                                            elsif style_function_group == '3'
                                                                assign icon = '7'
                                                            elsif style_function_group == '4'
                                                                assign icon = '8'
                                                            elsif style_function_group == '5'
                                                                assign icon = '9'
                                                            elsif style_function_group == '6'
                                                                assign icon = '4'
                                                            elsif style_function_group == '7'
                                                                assign icon = '5'
                                                            elsif style_function_group == '9'
                                                                assign icon = '2'
                                                            elsif style_function_group == '10'
                                                                assign icon = '3'
                                                            elsif style_function_group == '11'
                                                                assign icon = '6'
                                                            elsif style_function_group == '12'
                                                                assign icon = '10'
                                                            endif
                                                        -%}
                                                        {%- if style_function_group == '2' -%}
                                                            {% render 'icon-cart-special' %}
                                                        {%- else -%}
                                                            {% render 'icon-cart',
                                                                icon_cart: icon
                                                            %}
                                                        {%- endif -%}
                                                        <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
                                                        <div class="cart-count-bubble">
                                                            {%- if cart.item_count < 100 -%}
                                                                <span class="text" aria-hidden="true" data-cart-count>{{ cart.item_count }}</span>
                                                            {%- endif -%}
                                                            <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                                                        </div>
                                                    </a>
                                                {%- endif -%}
                                            </div>
                                        {%- endif -%}
                                    </div>
                            {%- endcase -%}
                        {%- endfor -%}
                    </div>
                </div>
            </div>
        {%- endif -%}
        {%- if block_main_menu.size > 0 -%}
            <div class="header-bottom{% if show_line_separator %} header-wrapper--border{% endif %} element-spacing"
                style="--spacing-top: {{ section.settings.bottom_padding_top | append: 'px' }};
                --spacing-bottom: {{ section.settings.bottom_padding_bottom | append: 'px' }};
                --spacing-top-mobile: {{ section.settings.bottom_padding_top | times: 0.5 | round: 0 | append: 'px' }};
                --spacing-bottom-mobile: {{ section.settings.bottom_padding_bottom | times: 0.5 | round: 0 | append: 'px' }};
                --spacing-top-tablet: {{ section.settings.bottom_padding_top | times: 0.75 | round: 0 | append: 'px' }};
                --spacing-bottom-tablet: {{ section.settings.bottom_padding_bottom | times: 0.75 | round: 0 | append: 'px' }};"
            >
                <div class="container container-{{ container }}">
                    {%- for block in section.blocks -%}
                        {%- case block.type -%}
                            {%- when 'main-menu' -%}
                                <div class="header-bottom--wrapper" style="--margin-right: {{ block.settings.margin_right | append: 'px' }};--margin-right-tablet: {{ block.settings.margin_right | times: 0.5 | round: 0 | append: 'px' }};">
                                    <div class="header-nav__menu header-bottom-center text-{{ align }}{% if show_resize_menu %} has-resize-menu{% endif %}{% if show_text_underline %} show-underline{% endif %}" id="HeaderNavigation" style="--menu-letter-spacing: {{ letter_spacing }};{% if show_text_underline %}--text-underline: {{ text_underline }};{% endif %}--spacing-left: {{ block.settings.padding_left | append: 'px' }};--spacing-right: {{ block.settings.padding_right | append: 'px' }};">
                                        {%- if menu != blank -%}
                                            <nav class="header__inline-menu{% if show_resize_menu %} flex justify-{{ align }}{% endif %}" data-navigation role="navigation" aria-label="{{ 'general.navigation.title' | t }}">
                                                {%- liquid
                                                    assign page_type = request.page_type | replace: 'customers/', ''
                                                    assign load = false
                                                    if page_type == 'search'
                                                        assign load = true
                                                    endif
                                                    assign listClass = 'list-menu list-menu--inline'
                                                    if align == 'left'
