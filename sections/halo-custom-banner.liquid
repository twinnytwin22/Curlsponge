{%- if section.blocks.size > 0 and section.settings.show_custom_banner -%}
    {%- liquid
        assign padding_top = section.settings.padding_top | append: 'px'
        assign padding_bottom = section.settings.padding_bottom | append: 'px'
        assign padding_top_tablet = section.settings.padding_top_tablet | append: 'px'
        assign padding_bottom_tablet = section.settings.padding_bottom_tablet | append: 'px'
        assign padding_top_mobile = section.settings.padding_top_mobile | append: 'px'
        assign padding_bottom_mobile = section.settings.padding_bottom_mobile | append: 'px'
        assign background_color_section = section.settings.background_color_section
        assign column = section.settings.column
        assign container = section.settings.container
        assign col = section.settings.col
        assign block_title = section.settings.block_title
        assign block_text = section.settings.block_text
        assign block_title_align = section.settings.block_title_align
        assign show_custom_banner_style = section.settings.show_custom_banner_style
        assign style = section.settings.style
        assign mobile_layout = section.settings.block_mobile_layout
        if show_custom_banner_style
            assign background_color = section.settings.background_color
            assign title_color = section.settings.title_color
            assign title_font = section.settings.title_font
            assign title_font_size = section.settings.title_font_size
            if title_font_size >= 60
                assign title_tablet_font_size = title_font_size | minus: 30 | append: 'px'
                assign title_font_size = title_font_size | append: 'px'
            else
                assign title_tablet_font_size = title_font_size | append: 'px'
                assign title_font_size = title_font_size | append: 'px'
            endif
            assign title_mobile_font_size = section.settings.title_mobile_font_size | append: 'px'
            assign title_font_weight = section.settings.title_font_weight
            assign title_text_transform = section.settings.title_text_transform
            assign text_color = section.settings.text_color
            assign text_font_size = section.settings.text_font_size | append: 'px'
            assign text_mobile_font_size = section.settings.text_mobile_font_size | append: 'px'
            assign text_font_weight = section.settings.text_font_weight
            assign text_text_transform = section.settings.text_text_transform
            assign transparent_button = section.settings.transparent_button
        endif
        assign image_border_radius = settings.image_border_radius
        assign image_effect = settings.image_effect
        assign show_hoverImage_1 = section.settings.show_hoverImage_1
        assign show_hoverImage_2 = section.settings.show_hoverImage_2

        if settings.show_dark_mode
            assign dark_mode_1 = false
            unless background_color_section == blank or background_color_section == '#fff' or background_color_section == '#FFF' or background_color_section == '#ffffff' or background_color_section == '#FFFFFF' or background_color_section == 'rgb(255,255,255)' or background_color_section == 'rgba(255,255,255,1)' or background_color_section == 'rgba(0,0,0,0)' or background_color_section == 'rgb(0,0,0,0)'
                assign dark_mode_1 = true
                assign dark_mode_background_color_section = section.settings.background_color_section
            endunless
            assign dark_mode_2 = false
            unless background_color == blank or background_color == '#fff' or background_color == '#FFF' or background_color == '#ffffff' or background_color == '#FFFFFF' or background_color == 'rgb(255,255,255)' or background_color == 'rgba(255,255,255,1)' or background_color == 'rgba(0,0,0,0)' or background_color == 'rgb(0,0,0,0)'
                assign dark_mode_2 = true
                if show_custom_banner_style
                    assign dark_mode_background_color = section.settings.background_color
                    assign dark_mode_title_color = section.settings.title_color
                    assign dark_mode_text_color = section.settings.text_color
                endif
            endunless
        endif
    -%}
    <div class="halo-block halo-banner custom-banner style-{{ style }} {% if show_hoverImage_1 %}show-button{% endif %} halo-block-padding halo-block-gradient{% unless dark_mode_1 %} dark-mode-1{% endunless %}{% unless dark_mode_2 %} dark-mode-2{% endunless %}" id="halo-custom-banner-{{ section.id }}"
        style="--padding-top: {{ padding_top }};
            --padding-bottom: {{ padding_bottom }};
            --padding-top-tablet: {{ padding_top_tablet }};
            --padding-bottom-tablet: {{ padding_bottom_tablet }};
            --padding-top-mobile: {{ padding_top_mobile }};
            --padding-bottom-mobile: {{ padding_bottom_mobile }};
            --bg-color: {{ background_color_section | default: settings.colors_bg }};
            {% if dark_mode_1 %}
                --dark-mode-bg-color: {{ dark_mode_background_color_section }};
            {% endif %}"
    >
        <div class="container container-{{ container }}{% if mobile_layout == 'swipe' %} container-no-padding{% endif %}">
            {%- if block_title != blank -%}
                <div class="halo-block-header">
                    <h3 class="title text-{{ block_title_align }}">
                        <span class="text">
                            {{ block_title | escape }}
                        </span>
                    </h3>
                    {%- if block_text != blank -%}
                        <span class="desc text-{{ block_title_align }}">
                            {{ block_text }}
                        </span>
                    {%- endif -%}
                </div>
            {%- endif -%}
            <div class="halo-block-content"
                {% if show_custom_banner_style %}
                    style="{% if title_font == 'font_1' %}
                        --title-font: var(--font-1-family);
                    {% elsif title_font == 'font_2' %}
                        --title-font: var(--font-2-family);
                    {% elsif title_font == 'font_3' %}
                        --title-font: var(--font-3-family);
                    {% endif %}
                    --title-font-size: {{ title_font_size }};
                    --title-tablet-font-size: {{ title_tablet_font_size }};
                    --title-mobile-font-size: {{ title_mobile_font_size }};
                    --title-font-weight: {{ title_font_weight }};
                    --title-text-transform: {{ title_text_transform }};
                    --title-color: {{ title_color }};
                    --text-font-size: {{ text_font_size }};
                    --text-mobile-font-size: {{ text_mobile_font_size }};
                    --text-font-weight: {{ text_font_weight }};
                    --text-text-transform: {{ text_text_transform }};
                    --text-color: {{ text_color }};
                    {% if dark_mode_2 %}
                        --dark-mode-title-color: {{ dark_mode_title_color }};
                        --dark-mode-text-color: {{ dark_mode_text_color }};
                    {% endif %}" 
                {% endif %}
            >
                <div class="halo-row halo-row--{{ col }} column-{{ column }} halo-row--{{ mobile_layout }} disable-scrollbar">
                    {%- for block in section.blocks -%}
                        {%- case block.type -%}
                            {%- when 'image' -%}
                                {%- liquid
                                    assign image = block.settings.image
                                    assign title = block.settings.title
                                    assign text = block.settings.text
                                    assign btn = block.settings.btn
                                    assign btn_text = block.settings.btn_text
                                    assign link = block.settings.link
                                    assign align = block.settings.align
                                    assign position = block.settings.position
                                -%}
                                <div class="halo-row-item item item-{{ block.id }}">
                                    <div class="custom-banner-item banner-item banner-item--large{% if show_hoverImage_2 %} image-{{ image_effect }}{% endif %} image-border-{{ image_border_radius }}{% if show_custom_banner_style %} custom-banner-item--style{% endif %}">
                                        <div class="img-box">
                                            <loading-banner class="loading-banner">
                                                {%- if image != blank -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} class="image image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {{ 1 | divided_by: image.aspect_ratio | times: 100 | append: '%' }}">
                                                        <img {% render 'lazyload', preview_img: image %}>
                                                        {%- if image_effect == 'liquid' -%}
                                                            <span class="hover"></span>
                                                        {%- endif -%}
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- else -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} class="image" {% if link != blank %} href="{{ link }}"{% endif %}>
                                                        {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- endif -%}
                                            </loading-banner>
                                        </div>
                                        {%- if title != blank or text != blank or btn_text != blank -%}
                                            <div class="content-box{% if style == '1' %} content-box--absolute content-box--{{ position }}{% elsif style == '3' or style == '4' %} content-box--custom{% endif %} text-{{ align }}{% if show_custom_banner_style %}{% if background_color != 'rgba(0,0,0,0)' and background_color != blank %} content-box--bg{% endif %}{% endif %}"
                                                style="--bg-color: {{ background_color }};
                                                {% if dark_mode_2 %}
                                                    --dark-mode-bg-color: {{ dark_mode_background_color }};
                                                {% endif %}"
                                            >
                                                {%- if title != blank -%}
                                                    <h4 class="banner-title{% if show_custom_banner_style %} typography__mobile typography__tablet typography__desktop{% endif %}">
                                                        {%- if link != blank -%}<a href="{{ link }}">{%- endif -%}{{ title | escape }}{%- if link != blank -%}</a>{%- endif -%}
                                                    </h4>
                                                {%- endif -%}
                                                {%- if text != blank -%}
                                                    <span class="banner-text{% if show_custom_banner_style %} typography__text{% endif %}">
                                                        {{ text }}
                                                    </span>
                                                {%- endif -%}
                                                {%- if btn_text != blank -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} {% if link != blank %}href="{{ link }}"{% endif %} class="banner-button{% if btn != 'link' %} button button-{{ btn }}{% if transparent_button %} button--transparent{% endif %}{% else %} button-link link text-underline link-underline{% endif %}{% if show_custom_banner_style %} typography__button{% endif %}">
                                                        <span class="text">
                                                            {{ btn_text | escape }}
                                                        </span>
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- endif -%}
                                            </div>
                                        {%- endif -%}
                                    </div>
                                </div>
                            {%- when 'image_with_overlay' -%}
                                {%- liquid
                                    assign image = block.settings.image
                                    assign title = block.settings.title
                                    assign text = block.settings.text
                                    assign link = block.settings.link
                                    assign align = block.settings.align
                                -%}
                                <div class="halo-row-item block-i-w-overlay item item-{{ block.id }}">
                                    <div class="custom-banner-item banner-item banner-item--large{% if show_hoverImage_2 %} image-overlay-2{% endif %} image-border-{{ image_border_radius }}{% if show_custom_banner_style %} custom-banner-item--style{% endif %}">
                                        <div class="img-box">
                                            <loading-banner class="loading-banner">
                                                {%- if image != blank -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} class="image image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {{ 1 | divided_by: image.aspect_ratio | times: 100 | append: '%' }}">
                                                        <img {% render 'lazyload', preview_img: image %}>
                                                        {%- if image_effect == 'liquid' -%}
                                                            <span class="hover"></span>
                                                        {%- endif -%}
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- else -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} class="image" {% if link != blank %} href="{{ link }}"{% endif %}>
                                                        {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- endif -%}
                                            </loading-banner>
                                        </div>
                                        {%- if title != blank or text != blank -%}
                                            <div class="content-box z-index-2 di-flex f-column w-s-normal text-{{ align }}{% if show_custom_banner_style %}{% if background_color != 'rgba(0,0,0,0)' and background_color != blank %} content-box--bg{% endif %}{% endif %}"
                                                style="
                                                    --top: 0; --left: 0; --right: 0; --bottom: 0;
                                                    --bg-color: {{ background_color }};
                                                    {% if dark_mode_2 %}
                                                        --dark-mode-bg-color: {{ dark_mode_background_color }};
                                                    {% endif %}
                                                "
                                            >
                                                {%- if title != blank -%}
                                                    <h4 class="banner-title{% if show_custom_banner_style %} typography__mobile typography__tablet typography__desktop{% endif %}">
                                                        {%- if link != blank -%}<a href="{{ link }}">{%- endif -%}{{ title | escape }}{%- if link != blank -%}</a>{%- endif -%}
                                                    </h4>
                                                {%- endif -%}
                                                {%- if text != blank -%}
                                                    <span class="banner-text{% if show_custom_banner_style %} typography__text{% endif %} block-rich-text h-link" style="--banner-m-t: auto; --banner-m-b: 0;">
                                                        {{ text }}
                                                    </span>
                                                {%- endif -%}
                                            </div>
                                        {%- endif -%}
                                    </div>
                                </div>
                        {%- endcase -%}
                    {%- endfor -%}
                </div>
            </div>
        </div>
    </div>
{%- endif -%}
{% schema %}
    {
        "name": "t:sections.custom-banner.name",
        "max_blocks": 6,
        "settings": [
            {
                "type": "header",
                "content": "t:sections.custom-banner.settings.header__1.content"
            },
            {
                "type": "checkbox",
                "id": "show_custom_banner",
                "label": "t:sections.custom-banner.settings.general.label__1",
                "default": true
            },
            {
                "type": "color_background",
                "id": "background_color_section",
                "label": "t:sections.general.settings.color.label__1"
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__1.content"
            },
            {
                "type": "range",
                "id": "padding_top",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "range",
                "id": "padding_bottom",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__2.content"
            },
            {
                "type": "range",
                "id": "padding_top_tablet",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 40
            },
            {
                "type": "range",
                "id": "padding_bottom_tablet",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 40
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__3.content"
            },
            {
                "type": "range",
                "id": "padding_top_mobile",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 30
            },
            {
                "type": "range",
                "id": "padding_bottom_mobile",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 30
            },
            {
                "type": "select",
                "id": "container",
                "label": "t:sections.general.settings.container.label__1",
                "default": "1770",
                "options": [
                {
                    "value": "1170",
                    "label": "t:sections.general.settings.container.options__1"
                },
                {
                    "value": "1370",
                    "label": "t:sections.general.settings.container.options__2"
                },
                {
                    "value": "1570",
                    "label": "t:sections.general.settings.container.options__3"
                },
                {
                    "value": "1770",
                    "label": "t:sections.general.settings.container.options__4"
                },
                {
                    "value": "1860",
                    "label": "t:sections.general.settings.container.options__5"
                },
                {
                    "value": "full",
                    "label": "t:sections.general.settings.container.options__6"
                }],
                "info": "t:sections.general.settings.container.info"
            },
            {
                "type": "select",
                "id": "column",
                "label": "t:sections.general.settings.grid.label__1",
                "default": "3",
                "options": [
                {
                    "value": "2",
                    "label": "2"
                },
                {
                    "value": "3",
                    "label": "3"
                },
                {
                    "value": "4",
                    "label": "4"
                },
                {
                    "value": "5",
                    "label": "5"
                }]
            },
            {
                "type": "text",
                "id": "block_title",
                "label": "t:sections.general.settings.block-title.label__1",
                "default": "Top Banner"
            },
            {
                "type": "text",
                "id": "block_text",
                "label": "t:sections.general.settings.block-title.label__2",
                "default": "Give customers details about the banner image(s) or content on the template."
            },
            {
                "type": "select",
                "id": "block_title_align",
                "label": "t:sections.general.settings.block-title.label__3",
                "options": [
                {
                    "value": "left",
                    "label": "t:sections.general.settings.block-title.options__1"
                },
                {
                    "value": "center",
                    "label": "t:sections.general.settings.block-title.options__2"
                },
                {
                    "value": "right",
                    "label": "t:sections.general.settings.block-title.options__3"
                }],
                "default": "center"
            },
            {
                "type": "select",
                "id": "block_mobile_layout",
