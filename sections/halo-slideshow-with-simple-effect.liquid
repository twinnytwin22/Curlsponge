{%- if section.blocks.size > 0 and section.settings.show_slideshow -%}
	{%- liquid
        assign padding_top = section.settings.padding_top | append: 'px'
        assign padding_bottom = section.settings.padding_bottom | append: 'px'
        assign padding_top_tablet = section.settings.padding_top_tablet | append: 'px'
        assign padding_bottom_tablet = section.settings.padding_bottom_tablet | append: 'px'
        assign padding_top_mobile = section.settings.padding_top_mobile | append: 'px'
        assign padding_bottom_mobile = section.settings.padding_bottom_mobile | append: 'px'
        assign container = section.settings.container
        assign block_arrows = section.settings.block_arrows
        assign block_arrows_bg = section.settings.block_arrows_bg
        assign block_arrows_color = section.settings.block_arrows_color
        assign block_arrows_border = section.settings.block_arrows_border
        assign block_dots = section.settings.block_dots
        assign block_dots_bg = section.settings.block_dots_bg
        assign block_dots_border = section.settings.block_dots_border
        assign block_autoplay = section.settings.block_autoplay
        assign block_autoplay_time = section.settings.block_autoplay_time | times: 1000
        assign has_script = false
        if section.blocks.size > 1
            assign has_script = true
        else
            assign block_video = section.blocks | where: "type", "video"
            if block_video.size > 0
                assign has_script = true
            endif
        endif
    -%}
    <div class="halo-block halo-banner slideshow slideshow-with-effect halo-block-padding" id="halo-slideshow-{{ section.id }}" data-slideshow-block-2 {% if has_script %} data-loader-script="{{ 'halo.slide-slideshow-2.js' | asset_url }}"{% endif %}
        style="--padding-top: {{ padding_top }};
            --padding-bottom: {{ padding_bottom }};
            --padding-top-tablet: {{ padding_top_tablet }};
            --padding-bottom-tablet: {{ padding_bottom_tablet }};
            --padding-top-mobile: {{ padding_top_mobile }};
            --padding-bottom-mobile: {{ padding_bottom_mobile }};
            {% if has_script %}
                --dot-bg: {{ block_dots_bg }};
                --dot-border: {{ block_dots_border }};
                --arrow-bg: {{ block_arrows_bg }};
                --arrow-border: {{ block_arrows_border }};
                --arrow-color: {{ block_arrows_color }};
            {% endif %}"
    >
    	{%- if container != 'full' -%}
	    	<div class="container container-{{ container }}">
	    {%- endif -%}
    		<div class="halo-block-content">
    			<div class="heroCarousel-wrapper clearfix">
    				<div class="heroCarousel{% if block_arrows %} heroCarousel-arrows{% endif %} halo-banner-wrapper"
	    				data-item-dots="{{ block_dots }}"
                        data-item-arrows="{{ block_arrows }}"
                        data-item-autoplay="{{ block_autoplay }}"
                        data-item-time="{{ block_autoplay_time }}"
                    >
                    	{%- for block in section.blocks -%}
                    		{%- liquid
                                if block.type == 'image'
                                    assign image = block.settings.image
                                    assign mobile_image = block.settings.mobile_image
                                elsif block.type == 'video'
                                    assign video = block.settings.video
                                    assign mobile_video = block.settings.mobile_video
                                    assign video_height = block.settings.video_height | append: 'px'
                                endif
                                assign title = block.settings.title
                                assign subtitle = block.settings.subtitle
                                assign text = block.settings.text
                                assign btn = block.settings.btn
                                assign btn_text = block.settings.btn_text
                                assign link = block.settings.link
                                assign position = block.settings.position
                                assign alignment = block.settings.alignment
                                assign show_background_overlay = block.settings.show_background_overlay
                                if show_background_overlay
                                    assign background_overlay = block.settings.background_overlay
                                endif
					            assign background_color = block.settings.background_color
					            assign title_color = block.settings.title_color
					            assign text_color = block.settings.text_color
					            assign transparent_button = block.settings.transparent_button
					            assign title_font = block.settings.title_font
					            assign title_font_size = block.settings.title_font_size
                                if title_font_size >= 60
                                    assign title_tablet_font_size = title_font_size | minus: 30 | append: 'px'
                                    assign title_font_size = title_font_size | append: 'px'
                                else
                                    assign title_tablet_font_size = title_font_size | append: 'px'
                                    assign title_font_size = title_font_size | append: 'px'
                                endif
					            assign title_mobile_font_size = block.settings.title_mobile_font_size | append: 'px'
					            assign title_font_weight = block.settings.title_font_weight
					            assign title_text_transform = block.settings.title_text_transform
					            assign text_font_size = block.settings.text_font_size | append: 'px'
					            assign text_font_weight = block.settings.text_font_weight
					            assign text_text_transform = block.settings.text_text_transform
                            -%}
                            <div class="item item-{{ block.id }}" data-slide-index="{{ forloop.index0 }}"
                                style="{% if title_font == 'font_1' %}
                                    --title-font: var(--font-1-family);
                                {% elsif title_font == 'font_2' %}
                                    --title-font: var(--font-2-family);
                                {% elsif title_font == 'font_3' %}
                                    --title-font: var(--font-3-family);
                                {% endif %}
                                --title-font-size: {{ title_font_size }};
                                --title-tablet-font-size: {{ title_tablet_font_size }};
                                --title-mobile-font-size: {{ title_mobile_font_size }};
                                --title-font-weight: {{ title_font_weight }};
                                --title-text-transform: {{ title_text_transform }};
                                --title-color: {{ title_color }};
                                --text-font-size: {{ text_font_size }};
                                --text-font-weight: {{ text_font_weight }};
                                --text-text-transform: {{ text_text_transform }};
                                --text-color: {{ text_color }};
                                {% if show_background_overlay %}
                                    --background-overlay: {{ background_overlay | color_modify: 'alpha', 0.3 }};
                                {% endif %}"
                            >
                            	<div class="heroCarousel-slide{% if forloop.first == true %} heroCarousel-slide--first{% endif %} banner-item" data-position="{{ position }}">
                            		<div class="heroCarousel-slide--wrapper">
                                        {%- if block.type == 'image' -%}
                                			<div class="heroCarousel-image img-box img-box--mobile">
                                			 	{%- if image != blank -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} class="image image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {% unless image.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: image.aspect_ratio | times: 100 | divided_by: 1.1 }}%{% else %}{{ 1 | divided_by: image.aspect_ratio | times: 100 }}%{% endunless %}">
                                                        <img srcset="{%- if image.width < 320 -%}{{ image | image_url: width: image.width }} 320w,{%- endif -%}
                                                            {%- if image.width >= 320 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                                                            {%- if image.width >= 740 -%}{{ image | image_url: width: 740 }} 740w,{%- endif -%} 
                                                            {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                                                            {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                            {%- if image.width >= 1370 -%}{{ image | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                            {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                            {%- if image.width >= 1770 -%}{{ image | image_url: width: 1770 }} 1770w,{%- endif -%}
                                                            {%- if image.width >= 1780 -%}{{ image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                            {%- if image.width >= 1880 -%}{{ image | image_url: width: 1880 }} 1880w,{%- endif -%}
                                                            {%- if image.width >= 2000 -%}{{ image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                            {%- if image.width >= 2800 -%}{{ image | image_url: width: 2800 }} 2800w{%- endif -%}"
                                                            alt="{{ image.alt | default: shop.name | escape }}" width="{{ image.width }}" height="{{ image.width | divided_by: image.aspect_ratio | round }}" 
                                                            {% unless image.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ image.presentation.focal_point }}"{% endunless %}
                                                            {%- if forloop.first == false -%}loading="lazy"{%- endif -%}
                                                        >
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- else -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} class="image" {% if link != blank %} href="{{ link }}"{% endif %}>
                                                        {% capture current %}{% cycle 1, 2 %}{% endcapture %}
                                                        {{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}   
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- endif -%}
                                                {%- if mobile_image != blank -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} class="image image-mobile image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {{ 1 | divided_by: mobile_image.aspect_ratio | times: 100 | append: '%' }}">
                                                        <img srcset="{%- if mobile_image.width < 320 -%}{{ mobile_image | image_url: width: mobile_image.width }} 320w,{%- endif -%}
                                                            {%- if mobile_image.width >= 320 -%}{{ mobile_image | image_url: width: 375 }} 375w,{%- endif -%}
                                                            {%- if mobile_image.width >= 750 -%}{{ mobile_image | image_url: width: 750 }} 750w,{%- endif -%}
                                                            {%- if mobile_image.width >= 1100 -%}{{ mobile_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                            {%- if mobile_image.width >= 1370 -%}{{ mobile_image | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                            {%- if mobile_image.width >= 1500 -%}{{ mobile_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                            {%- if mobile_image.width >= 1780 -%}{{ mobile_image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                            {%- if mobile_image.width >= 2000 -%}{{ mobile_image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                            {%- if mobile_image.width >= 2800 -%}{{ mobile_image | image_url: width: 2800 }} 2800w{%- endif -%}"
                                                            alt="{{ mobile_image.alt | default: shop.name | escape }}" width="{{ mobile_image.width }}" height="{{ mobile_image.width | divided_by: mobile_image.aspect_ratio | round}}"
                                                            {%- if forloop.first == false -%}loading="lazy"{%- endif -%}
                                                        >
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- else -%}
                                                    {%- if image != blank -%}
                                                        <{% if link != blank %}a{% else %}div{% endif %} class="image image-mobile image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {% unless image.presentation.focal_point == '50.0% 50.0%' %}100%{% else %}{{ 1 | divided_by: image.aspect_ratio | times: 100 }}%{% endunless %}">
                                                            <img srcset="{%- if image.width < 320 -%}{{ image | image_url: width: image.width }} 320w,{%- endif -%}
                                                                {%- if image.width >= 320 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                                                                {%- if image.width >= 740 -%}{{ image | image_url: width: 740 }} 740w,{%- endif -%} 
                                                                {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                                                                {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                                {%- if image.width >= 1370 -%}{{ image | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                                {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                                {%- if image.width >= 1770 -%}{{ image | image_url: width: 1770 }} 1770w,{%- endif -%}
                                                                {%- if image.width >= 1780 -%}{{ image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                                {%- if image.width >= 1880 -%}{{ image | image_url: width: 1880 }} 1880w,{%- endif -%}
                                                                {%- if image.width >= 2000 -%}{{ image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                                {%- if image.width >= 2800 -%}{{ image | image_url: width: 2800 }} 2800w{%- endif -%}"
                                                                alt="{{ image.alt | default: shop.name | escape }}" width="{{ image.width }}" height="{{ image.width | divided_by: image.aspect_ratio | round }}" 
                                                                {% unless image.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ image.presentation.focal_point }}"{% endunless %}
                                                                {%- if forloop.first == false -%}loading="lazy"{%- endif -%}
                                                            >
                                                        </{% if link != blank %}a{% else %}div{% endif %}>
                                                    {%- else -%}
                                                        <{% if link != blank %}a{% else %}div{% endif %} class="image image-mobile" {% if link != blank %} href="{{ link }}"{% endif %}>
                                                            {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                                        </{% if link != blank %}a{% else %}div{% endif %}>
                                                    {%- endif -%}
                                                {%- endif -%}
                                			</div>
                                        {%- elsif block.type == 'video' -%}
                                            <div class="heroCarousel-video video-only video-box video-box--mobile" style="height: {{ video_height }};">
                                                {%- if video != blank -%}
                                                    <div class="video">
                                                        <video autoplay muted disablepictureinpicture playsinline loop>
                                                            <source src="{{ video }}" type="video/mp4">
                                                            Your browser does not support the video tag.
                                                        </video>
                                                    </div>
                                                {%- else -%}
                                                    <div class="video">
                                                        <video autoplay muted disablepictureinpicture playsinline loop>
                                                            <source src="https://cdn.shopify.com/videos/c/o/v/4d80eadf7ddb45beb056304410e68110.mp4" type="video/mp4">
                                                            Your browser does not support the video tag.
                                                        </video>
                                                    </div>
                                                {%- endif -%}
                                                {%- if mobile_video != blank -%}
                                                    <div class="video video-mobile">
                                                        <video autoplay muted disablepictureinpicture playsinline loop>
                                                            <source src="{{ mobile_video }}" type="video/mp4">
                                                            Your browser does not support the video tag.
                                                        </video>
                                                    </div>
                                                {%- else -%}
                                                    {%- if video != blank -%}
                                                        <div class="video video-mobile">
                                                            <video autoplay muted disablepictureinpicture playsinline loop>
                                                                <source src="{{ video }}" type="video/mp4">
                                                                Your browser does not support the video tag.
                                                            </video>
                                                        </div>
                                                    {%- else -%}
                                                        <div class="video video-mobile">
                                                            <video autoplay muted disablepictureinpicture playsinline loop>
                                                                <source src="https://cdn.shopify.com/videos/c/o/v/b239d7ed48e7477da9d58c79bae2a641.mp4" type="video/mp4">
                                                                Your browser does not support the video tag.
                                                            </video>
                                                        </div>
                                                    {%- endif -%}
                                                {%- endif -%}
                                            </div>
                                        {%- endif -%}
                            			{%- if subtitle != blank or title != blank or text != blank or btn_text != blank -%}
                            				<div class="heroCarousel-content content-box content-box--absolute content-box--full content-box--1570 content-box--{{ position }} text-{{ alignment }}{% if background_color != 'rgba(0,0,0,0)' and background_color != blank %} content-box--bg{% endif %}"
                                                style="--bg-color: {{ background_color }};"
                                            >
                            					{%- if subtitle != blank -%}
                                                    <span class="heroCarousel-subtitle banner-subtitle typography__subtitle">
                                                        {{ subtitle | escape }}
                                                    </span>
                                                {%- endif -%}
                            					{%- if title != blank -%}
                                                    <h4 class="heroCarousel-title banner-title typography__mobile typography__tablet typography__desktop">
                                                        {{ title | escape }}
                                                    </h4>
                                                {%- endif -%}
                                                {%- if text != blank -%}
                                                    <span class="heroCarousel-description banner-text typography__text">
                                                        {{ text }}
                                                    </span>
                                                {%- endif -%}
                                                {%- if btn_text != blank -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} {% if link != blank %} href="{{ link }}"{% endif %} class="heroCarousel-action banner-button button button-{{ btn }}{% if transparent_button %} button--transparent{% endif %}">
                                                        <span class="text">
                                                            {{ btn_text | escape }}
                                                        </span>
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- endif -%}
                            				</div>
                            			{%- endif -%}
                            		</div>
                            	</div>
                            </div>
                    	{%- endfor -%}
                    </div>
    			</div>
    		</div>
    	{%- if container != 'full' -%}
	    	</div>
	    {%- endif -%}
    </div>
{%- endif -%}
{% schema %}
	{
        "name": "t:sections.slideshow.name__3",
        "settings": [
            {
                "type": "header",
                "content": "t:sections.slideshow.settings.header__1.content"
            },
            {
                "type": "checkbox",
                "id": "show_slideshow",
                "label": "t:sections.slideshow.settings.general.label__1",
                "default": true
            },
            {
                "type": "select",
                "id": "container",
                "label": "t:sections.general.settings.container.label__1",
                "default": "full",
                "options": [
                {
                    "value": "1170",
                    "label": "t:sections.general.settings.container.options__1"
                },
                {
                    "value": "1370",
                    "label": "t:sections.general.settings.container.options__2"
                },
                {
                    "value": "1570",
                    "label": "t:sections.general.settings.container.options__3"
                },
                {
                    "value": "1770",
                    "label": "t:sections.general.settings.container.options__4"
                },
                {
                    "value": "1860",
                    "label": "t:sections.general.settings.container.options__5"
                },
                {
                    "value": "full",
                    "label": "t:sections.general.settings.container.options__6"
                }],
                "info": "t:sections.general.settings.container.info"
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__1.content"
            },
            {
                "type": "range",
                "id": "padding_top",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "range",
                "id": "padding_bottom",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__2.content"
            },
            {
                "type": "range",
                "id": "padding_top_tablet",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "range",
                "id": "padding_bottom_tablet",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__3.content"
            },
            {
                "type": "range",
                "id": "padding_top_mobile",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "range",
                "id": "padding_bottom_mobile",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "header",
                "content": "t:sections.slideshow.settings.header__2.content"
            },
            {
                "type": "checkbox",
                "id": "block_autoplay",
                "label": "t:sections.general.settings.slider.label__5",
                "default": true
            },
            {
                "type": "range",
                "id": "block_autoplay_time",
                "label": "t:sections.general.settings.slider.label__6",
                "unit": "t:sections.general.settings.slider.unit",
                "min": 5,
                "max": 180,
                "step": 5,
                "default": 5
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.slider.paragraph__1.content"
            },
            {
                "type": "checkbox",
                "id": "block_dots",
                "label": "t:sections.general.settings.slider.label__2",
                "default": false
            },
            {
                "type": "color_background",
                "id": "block_dots_bg",
                "label": "t:sections.general.settings.color.label__1"
            },
            {
                "type": "color",
                "id": "block_dots_border",
                "label": "t:sections.general.settings.color.label__5"
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.slider.paragraph__2.content"
            },
            {
                "type": "checkbox",
                "id": "block_arrows",
                "label": "t:sections.general.settings.slider.label__3",
                "default": true
            },
            {
                "type": "color_background",
                "id": "block_arrows_bg",
                "label": "t:sections.general.settings.color.label__1"
            },
            {
                "type": "color",
                "id": "block_arrows_color",
                "label": "t:sections.general.settings.color.label__2"
            },
            {
                "type": "color",
                "id": "block_arrows_border",
                "label": "t:sections.general.settings.color.label__5"
            }
        ],
        "blocks": [
            {
                "type": "image",
                "name": "t:sections.slideshow.blocks.image.name__1",
                "settings": [
                {
                    "type": "header",
                    "content": "t:sections.slideshow.blocks.image.settings.header__1.content"
                },		  	
                {
                    "type": "image_picker",
                    "id": "image",
                    "label": "t:sections.general.settings.image.label__1"
                },
                {
                    "type": "image_picker",
                    "id": "mobile_image",
                    "label": "t:sections.general.settings.image.label__2",
                    "info": "t:sections.general.settings.image.info"
                },
                {
                    "type": "header",
                    "content": "t:sections.slideshow.blocks.image.settings.header__2.content"
                },
                {
                    "type": "text",
                    "id": "title",
                    "label": "t:sections.general.settings.content.label__1",
                    "default": "Image banner"
                },
                {
                    "type": "text",
                    "id": "subtitle",
                    "label": "t:sections.general.settings.content.label__3"
                },
                {
                    "type": "text",
                    "id": "text",
                    "label": "t:sections.general.settings.content.label__2",
                    "default": "Give customers details about the banner image(s) or content on the template."
                },
                {
                    "type": "select",
                    "id": "btn",
                    "label": "t:sections.general.settings.button.label__1",
                    "default": "2",
                    "options": [
                    {
                        "value": "1",
                        "label": "t:sections.general.settings.button.options__1"
                    },
                    {
                        "value": "2",
                        "label": "t:sections.general.settings.button.options__2"
                    },
                    {
                        "value": "3",
                        "label": "t:sections.general.settings.button.options__3"
                    },
                    {
                        "value": "4",
                        "label": "t:sections.general.settings.button.options__4"
                    },
                    {
                        "value": "5",
                        "label": "t:sections.general.settings.button.options__5"
                    },
                    {
                        "value": "6",
                        "label": "t:sections.general.settings.button.options__9"
