{%- if section.settings.show_banner_with_custom_link -%}
    {%- liquid
        assign padding_top = section.settings.padding_top | append: 'px'
        assign padding_bottom = section.settings.padding_bottom | append: 'px'
        assign padding_top_tablet = section.settings.padding_top_tablet | append: 'px'
        assign padding_bottom_tablet = section.settings.padding_bottom_tablet | append: 'px'
        assign padding_top_mobile = section.settings.padding_top_mobile | append: 'px'
        assign padding_bottom_mobile = section.settings.padding_bottom_mobile | append: 'px'
        assign container = section.settings.container
        assign layout_style = section.settings.style
        assign image = section.settings.image
        assign mobile_image = section.settings.mobile_image
        assign subtitle = section.settings.subtitle
        assign title = section.settings.title
        assign align = section.settings.align
        assign title_color = section.settings.title_color
        assign text_color = section.settings.text_color
        assign sub_title_font = section.settings.sub_title_font
        assign sub_title_font_size = section.settings.sub_title_font_size | append: 'px'
        assign sub_title_font_weight = section.settings.sub_title_font_weight
        assign sub_title_text_transform = section.settings.sub_title_text_transform
        assign title_font = section.settings.title_font
        assign title_font_size = section.settings.title_font_size
        if title_font_size >= 60
            assign title_tablet_font_size = title_font_size | minus: 30 | append: 'px'
            assign title_font_size = title_font_size | append: 'px'
        else
            assign title_tablet_font_size = title_font_size | append: 'px'
            assign title_font_size = title_font_size | append: 'px'
        endif
        assign title_mobile_font_size = section.settings.title_mobile_font_size | append: 'px'
        assign title_font_weight = section.settings.title_font_weight
        assign title_text_transform = section.settings.title_text_transform
        assign show_countdown = section.settings.show_countdown
        assign countdown_time = section.settings.countdown_time
        assign block_title = section.settings.block_title
        assign block_text = section.settings.block_text
        assign block_title_align = section.settings.block_title_align
    -%}
    <div class="halo-block halo-banner fullwidth-banner halo-customlink-banner style-{{ layout_style }} halo-block-padding" id="halo-customlink-banner-{{ section.id }}"
        style="--padding-top: {{ padding_top }};
            --padding-bottom: {{ padding_bottom }};
            --padding-top-tablet: {{ padding_top_tablet }};
            --padding-bottom-tablet: {{ padding_bottom_tablet }};
            --padding-top-mobile: {{ padding_top_mobile }};
            --padding-bottom-mobile: {{ padding_bottom_mobile }};"
    >
        <div class="container container-{{ container }}">
            {%- if block_title != blank or block_text != blank -%}
                <div class="halo-block-header">
                    {%- if block_title != blank -%}
                        <h3 class="title text-{{ block_title_align }}">
                            <span class="text">
                                {{ block_title | escape }}
                            </span>
                        </h3>
                    {%- endif -%}
                    {%- if block_text != blank -%}
                        <span class="desc text-{{ block_title_align }}">
                            {{ block_text }}
                        </span>
                    {%- endif -%}
                </div>
            {%- endif -%}
            <div class="halo-block-content">
                <div class="halo-banner-wrapper">
                    <div class="item">
                        <div class="banner-item">
                            <div class="img-box img-box--mobile">
                                <loading-banner class="loading-banner">
                                    {%- if image != blank -%}
                                        <{% if link != blank %}a{% else %}div{% endif %} class="image image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {% unless image.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: image.aspect_ratio | times: 100 | divided_by: 1.1 }}%{% else %}{{ 1 | divided_by: image.aspect_ratio | times: 100 }}%{% endunless %}">
                                            <img srcset="{%- if image.width < 320 -%}{{ image | image_url: width: image.width }} 320w,{%- endif -%}
                                                {%- if image.width >= 320 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                                                {%- if image.width >= 740 -%}{{ image | image_url: width: 740 }} 740w,{%- endif -%} 
                                                {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                                                {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                {%- if image.width >= 1370 -%}{{ image | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                {%- if image.width >= 1770 -%}{{ image | image_url: width: 1770 }} 1770w,{%- endif -%}
                                                {%- if image.width >= 1780 -%}{{ image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                {%- if image.width >= 1880 -%}{{ image | image_url: width: 1880 }} 1880w,{%- endif -%}
                                                {%- if image.width >= 2000 -%}{{ image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                {%- if image.width >= 2800 -%}{{ image | image_url: width: 2800 }} 2800w{%- endif -%}"
                                                alt="{{ image.alt | default: shop.name | escape }}" width="{{ image.width }}" height="{{ image.width | divided_by: image.aspect_ratio | round }}" 
                                                {% unless image.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ image.presentation.focal_point }}"{% endunless %}
                                                loading="lazy"
                                            >
                                        </{% if link != blank %}a{% else %}div{% endif %}>
                                    {%- else -%}
                                        <{% if link != blank %}a{% else %}div{% endif %} class="image" {% if link != blank %} href="{{ link }}"{% endif %}>
                                            {% capture current %}{% cycle 1, 2 %}{% endcapture %}
                                            {{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}   
                                        </{% if link != blank %}a{% else %}div{% endif %}>
                                    {%- endif -%}
                                    {%- if mobile_image != blank -%}
                                        <{% if link != blank %}a{% else %}div{% endif %} class="image image-mobile image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {{ 1 | divided_by: mobile_image.aspect_ratio | times: 100 | append: '%' }}">
                                            <img srcset="{%- if mobile_image.width < 320 -%}{{ mobile_image | image_url: width: mobile_image.width }} 320w,{%- endif -%}
                                                {%- if mobile_image.width >= 320 -%}{{ mobile_image | image_url: width: 375 }} 375w,{%- endif -%}
                                                {%- if mobile_image.width >= 750 -%}{{ mobile_image | image_url: width: 750 }} 750w,{%- endif -%}
                                                {%- if mobile_image.width >= 1100 -%}{{ mobile_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                {%- if mobile_image.width >= 1370 -%}{{ mobile_image | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                {%- if mobile_image.width >= 1500 -%}{{ mobile_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                {%- if mobile_image.width >= 1780 -%}{{ mobile_image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                {%- if mobile_image.width >= 2000 -%}{{ mobile_image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                {%- if mobile_image.width >= 2800 -%}{{ mobile_image | image_url: width: 2800 }} 2800w{%- endif -%}"
                                                alt="{{ mobile_image.alt | default: shop.name | escape }}" width="{{ mobile_image.width }}" height="{{ mobile_image.width | divided_by: mobile_image.aspect_ratio | round}}"
                                                loading="lazy"
                                            >
                                        </{% if link != blank %}a{% else %}div{% endif %}>
                                    {%- else -%}
                                        {%- if image != blank -%}
                                            <{% if link != blank %}a{% else %}div{% endif %} class="image image-mobile image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {% unless image.presentation.focal_point == '50.0% 50.0%' %}100%{% else %}{{ 1 | divided_by: image.aspect_ratio | times: 100 }}%{% endunless %}">
                                                <img srcset="{%- if image.width < 320 -%}{{ image | image_url: width: image.width }} 320w,{%- endif -%}
                                                    {%- if image.width >= 320 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                                                    {%- if image.width >= 740 -%}{{ image | image_url: width: 740 }} 740w,{%- endif -%} 
                                                    {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                                                    {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                    {%- if image.width >= 1370 -%}{{ image | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                    {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                    {%- if image.width >= 1770 -%}{{ image | image_url: width: 1770 }} 1770w,{%- endif -%}
                                                    {%- if image.width >= 1780 -%}{{ image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                    {%- if image.width >= 1880 -%}{{ image | image_url: width: 1880 }} 1880w,{%- endif -%}
                                                    {%- if image.width >= 2000 -%}{{ image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                    {%- if image.width >= 2800 -%}{{ image | image_url: width: 2800 }} 2800w{%- endif -%}"
                                                    alt="{{ image.alt | default: shop.name | escape }}" width="{{ image.width }}" height="{{ image.width | divided_by: image.aspect_ratio | round }}" 
                                                    {% unless image.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ image.presentation.focal_point }}"{% endunless %}
                                                    loading="lazy"
                                                >
                                            </{% if link != blank %}a{% else %}div{% endif %}>
                                        {%- else -%}
                                            <{% if link != blank %}a{% else %}div{% endif %} class="image image-mobile" {% if link != blank %} href="{{ link }}"{% endif %}>
                                                {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                            </{% if link != blank %}a{% else %}div{% endif %}>
                                        {%- endif -%}
                                    {%- endif -%}
                                </loading-banner>
                                <div class="video-banner">
                                    <a href="javascript:void(0)" class="halo-modal-close clearfix" data-close-video role="button">
                                        <span class="visually-hidden">{{ 'general.common.close' | t }}</span>
                                        {% render 'icon-close' %}
                                    </a>
                                    <div class="modal-content-video"></div>
                                </div>
                            </div>
                            {%- if title != blank or subtitle != blank -%}
                                <div class="content-box content-box--absolute content-box--full text-center"
                                    style="{% if sub_title_font == 'font_1' %}
                                    --sub-title-font: var(--font-1-family);
                                    {% elsif sub_title_font == 'font_2' %}
                                    --sub-title-font: var(--font-2-family);
                                    {% elsif sub_title_font == 'font_3' %}
                                        --sub-title-font: var(--font-3-family);
                                    {% endif %}
                                    --sub-title-font-size: {{ sub_title_font_size }};
                                    --sub-title-font-weight: {{ sub_title_font_weight }};
                                    --sub-title-text-transform: {{ sub_title_text_transform }};
                                    {% if title_font == 'font_1' %}
                                        --title-font: var(--font-1-family);
                                    {% elsif title_font == 'font_2' %}
                                        --title-font: var(--font-2-family);
                                    {% elsif title_font == 'font_3' %}
                                        --title-font: var(--font-3-family);
                                    {% endif %}
                                    --title-font-size: {{ title_font_size }};
                                    --title-tablet-font-size: {{ title_tablet_font_size }};
                                    --title-mobile-font-size: {{ title_mobile_font_size }};
                                    --title-font-weight: {{ title_font_weight }};
                                    --title-text-transform: {{ title_text_transform }};
                                    --title-color: {{ title_color }};
                                    --text-color: {{ text_color }};"
                                >
                                    {%- if subtitle != blank -%}
                                        <h5 class="sub-title">
                                            {{ subtitle | escape }}
                                        </h5>
                                    {%- endif -%}
                                    {%- if title != blank -%}
                                        <h4 class="banner-title typography__mobile typography__tablet typography__desktop">
                                            {{ title | escape }}
                                        </h4>
                                    {%- endif -%}
                                    {%- if show_countdown -%}
                                        <div class="banner-view-countDown" data-countdown-banner>
                                            <div class="countDown countdown_{{ section.id }} clearfix" data-countdown-id="{{ section.id }}" data-countdown="{{ countdown_time }}">
                                                <span class="item animated-loading">
                                                    <span class="num">151</span>
                                                    <span class="text">d</span>
                                                </span>
                                                <span class="item animated-loading">
                                                    <span class="num">8</span>
                                                </span>
                                                <span class="item minnute animated-loading">
                                                    <span class="num">23</span>
                                                </span>
                                                <span class="item second animated-loading">
                                                    <span class="num" >00</span>
                                                </span>
                                            </div>
                                        </div>
                                    {%- endif -%}

                                    {%- if layout_style == '2' or layout_style == '3'-%}
                                        {%- if section.blocks.size > 0 -%}
                                            <div class="wrapper-button">
                                                {%- for block in section.blocks -%}
                                                    {%- liquid
                                                        assign btn = block.settings.btn
                                                        assign btn_text = block.settings.btn_text
                                                        assign link = block.settings.link
                                                    -%}
                                                    {%- if btn_text != blank -%}
                                                        {%- if block.settings.button_option == 'link' -%}
                                                            <{% if link != blank %}a{% else %}div{% endif %} {% if link != blank %} href="{{ link }}"{% endif %} class="banner-button{% if layout_style == '2' %} button button-{{ btn }}{% else %} link text-underline{% endif %}">
                                                                {%- if layout_style == '3' -%}
                                                                    <span class="text">
                                                                {%- endif -%}
                                                                    {{ btn_text | escape }}
                                                                {%- if layout_style == '3' -%}
                                                                    </span>
                                                                {%- endif -%}
                                                            </{% if link != blank %}a{% else %}div{% endif %}>
                                                        {%- else -%}
                                                            <a class="banner-button banner-button{% if layout_style == '2' %} button button-{{ btn }}{% else %} link text-underline{% endif %}{% if block.settings.button_option == 'video_youtube' %} video_youtube{% else %} video_mp4{% endif %}" href="javascript:void(0);" aria-label="link" data-video-banner data-video="{% if block.settings.button_option == 'video_youtube' %}{{ block.settings.video_link }}{% else %}{{ block.settings.video_mp4_link }}{% endif %}" role="button">
                                                                {% render 'icon-pause-2-button' %} 
                                                                {%- if layout_style == '3' -%}
                                                                    <span class="text">
                                                                {%- endif -%}
                                                                    {{ btn_text | escape }}
                                                                {%- if layout_style == '3' -%}
                                                                    </span>
                                                                {%- endif -%}
                                                            </a>
                                                        {%- endif -%}
                                                    {%- endif -%}
                                                {%- endfor -%}
                                            </div>
                                        {%- endif -%}
                                    {%- endif -%}
                                </div>
                            {%- endif -%}
                            {%- if layout_style == '1' -%}
                                {%- if section.blocks.size > 0 -%}
                                    <div class="wrapper-button">
                                        {%- for block in section.blocks -%}
                                            {%- liquid
                                                assign btn = block.settings.btn
                                                assign btn_text = block.settings.btn_text
                                                assign link = block.settings.link
                                            -%}
                                            {%- if btn_text != blank -%}
                                                {%- if block.settings.button_option == 'link' -%}
                                                    <{% if link != blank %}a{% else %}div{% endif %} {% if link != blank %} href="{{ link }}"{% endif %} class="banner-button button button-{{ btn }}">
                                                        {{ btn_text | escape }}
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
                                                {%- else -%}
                                                    <a class="banner-button button button-{{ btn }}{% if block.settings.button_option == 'video_youtube' %} video_youtube{% else %} video_mp4{% endif %}" href="javascript:void(0);" aria-label="link" data-video-banner data-video="{% if block.settings.button_option == 'video_youtube' %}{{ block.settings.video_link }}{% else %}{{ block.settings.video_mp4_link }}{% endif %}" role="button">
                                                        {% render 'icon-pause-2-button' %} {{ btn_text | escape }}
                                                    </a>
                                                {%- endif -%}
                                            {%- endif -%}
                                        {%- endfor -%}
                                    </div>
                                {%- endif -%}
                            {%- endif -%}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{%- endif -%}
{% schema %}
    {
        "name": "t:sections.banner-custom-link.name",
        "max_blocks":12,
        "settings": [
            {
                "type": "header",
                "content": "t:sections.banner-custom-link.settings.header__1.content"
            },
            {
                "type": "checkbox",
                "id": "show_banner_with_custom_link",
                "label": "t:sections.banner-custom-link.settings.general.label__1",
                "default": true
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__1.content"
            },
            {
                "type": "range",
                "id": "padding_top",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "range",
                "id": "padding_bottom",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__2.content"
            },
            {
                "type": "range",
                "id": "padding_top_tablet",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 40
            },
            {
                "type": "range",
                "id": "padding_bottom_tablet",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 40
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__3.content"
            },
            {
                "type": "range",
                "id": "padding_top_mobile",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 30
            },
            {
                "type": "range",
                "id": "padding_bottom_mobile",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 30
            },
            {
                "type": "select",
                "id": "container",
                "label": "t:sections.general.settings.container.label__1",
                "default": "1770",
                "options": [
                {
                    "value": "1170",
                    "label": "t:sections.general.settings.container.options__1"
                },
                {
                    "value": "1370",
                    "label": "t:sections.general.settings.container.options__2"
                },
                {
                    "value": "1570",
                    "label": "t:sections.general.settings.container.options__3"
                },
                {
                    "value": "1770",
                    "label": "t:sections.general.settings.container.options__4"
                },
                {
                    "value": "1860",
                    "label": "t:sections.general.settings.container.options__5"
                },
                {
                    "value": "full",
                    "label": "t:sections.general.settings.container.options__6"
                }],
                "info": "t:sections.general.settings.container.info"
            },
            {
                "type" : "select",
                "id" : "style",
                "label" : "t:sections.general.settings.style.label__1",
                "default": "1",
                "options": [
                {
                    "value": "1",
                    "label": "1"
                },
                {
                    "value": "2",
                    "label": "2"
                },
                {
                    "value": "3",
                    "label": "3"
                }]
            },
            {
                "type": "text",
                "id": "block_title",
                "label": "t:sections.general.settings.block-title.label__1"
            },
            {
                "type": "text",
                "id": "block_text",
                "label": "t:sections.general.settings.block-title.label__2"
            },
            {
                "type": "select",
                "id": "block_title_align",
                "label": "t:sections.general.settings.block-title.label__3",
                "options": [
                {
                    "value": "left",
                    "label": "t:sections.general.settings.block-title.options__1"
                },
                {
                    "value": "center",
                    "label": "t:sections.general.settings.block-title.options__2"
                },
                {
                    "value": "right",
                    "label": "t:sections.general.settings.block-title.options__3"
                }],
                "default": "center"
            },
            {
                "type": "header",
                "content": "t:sections.banner-custom-link.settings.header__2.content"
            }, 
            {
                "type": "image_picker",
                "id": "image",
                "label": "t:sections.general.settings.image.label__1"
            },
            {
                "type": "image_picker",
                "id": "mobile_image",
                "label": "t:sections.general.settings.image.label__2",
                "info": "t:sections.general.settings.image.info"
            },
            {
                "type": "header",
                "content": "t:sections.banner-custom-link.settings.header__3.content"
            },
            {
                "type": "text",
                "id": "title",
                "label": "t:sections.general.settings.content.label__1",
                "default":"SHOP EVERYTHING OUTDOOR"
            },
            {
                "type": "text",
                "id": "subtitle",
                "label": "t:sections.general.settings.content.label__3",
                "default":"HALO INTERIORS"
            },
            {
                "type": "header",
                "content": "t:sections.banner-custom-link.settings.header__4.content"
            },
            {
                "type": "checkbox",
                "label": "t:sections.general.settings.countdown.label__1",
                "id": "show_countdown",
                "default": false
            },
            {
                "type": "text",
                "id": "countdown_time",
                "label": "t:sections.general.settings.countdown.label__2",
                "default": "Dec 31, 2021 18:00:00",
                "info": "t:sections.general.settings.countdown.info"
            },
            {
                "type": "header",
                "content": "t:sections.banner-custom-link.settings.header__5.content"
            },
            {
                "type": "paragraph",
                "content": "t:settings_schema.typography.settings.paragraph__1.content"
            },
            {
                "type": "select",
                "id": "title_font",
                "label": "t:settings_schema.typography.settings.font.label",
                "options": [
                {
                    "value": "font_1",
                    "label": "t:settings_schema.typography.settings.font.options__1"
                },
                {
                    "value": "font_2",
                    "label": "t:settings_schema.typography.settings.font.options__2"
                },
                {
                    "value": "font_3",
                    "label": "t:settings_schema.typography.settings.font.options__3"
                }],
                "default": "font_2"
            },
            {
                "type": "range",
                "id": "title_font_size",
                "label": "t:settings_schema.typography.settings.font_size.label",
                "min": 10,
                "max": 80,
                "step": 1,
                "unit": "t:settings_schema.typography.settings.font_size.unit",
                "default": 80
            },
            {
                "type": "range",
                "id": "title_mobile_font_size",
                "label": "t:settings_schema.typography.settings.font_size.label__2",
                "min": 10,
                "max": 40,
                "step": 1,
                "unit": "t:settings_schema.typography.settings.font_size.unit",
                "default": 30
